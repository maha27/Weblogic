#How to encypt passwords in Dads file in OHS - Oracle Http Server

export PATH=$ORACLE_HOME/perl/bin:$PATH
export LD_LIBRARY_PATH=$ORACLE_HOME/lib:$LD_LIBRARY PATH 
export PERL5LIB=$ORACLE_HOME/perl/lib

cd $ORACLE_HOME/bin

perl dadTool.pl -f $ORACLE_INSTANCE/config/OHS/ohs1/test/dads_<FILENAME>.conf

